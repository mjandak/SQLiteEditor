<Window x:Class="Common.PSWDWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Enter password" Height="300" Width="300">
    <Grid>
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="81,67,0,0" Name="passwordBox" VerticalAlignment="Top" Width="185" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="144,133,0,0" Name="cancelBtn" VerticalAlignment="Top" Width="75" Click="cancelBtn_Click" />
        <Button IsDefault="True" Content="OK" Height="23" HorizontalAlignment="Left" Margin="63,133,0,0" Name="OKBtn" VerticalAlignment="Top" Width="75" Click="OKBtn_Click" />
        <Label Content="Password:" Height="28" HorizontalAlignment="Left" Margin="12,67,0,0" Name="label1" VerticalAlignment="Top" />
    </Grid>
</Window>
